            <th>{{dur_1}} seconds</th>
            <th>{{dur_2}} seconds</th>
          </tr>
        </table>
      </div>
      <div id="spinner" style="width:400px; margin-left:600px"></div>

      <form method="post" enctype="multipart/form-data" action="/result" id = "dynamic_result">
        <input type="submit" value="More details" id="button4" class = "midd_result">
      </form>
    </div>
  </div>
