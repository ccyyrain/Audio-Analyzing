  <div>
    <h1 id="result_h" style="float: right; margin-right:150px">Analysis result </h1>
    <div>
      <div id = "table1" class="resultFont" style="float: right">
        <table id = "table_1" style="margin-left:300px; color: #FFFFFF">
          <tr>
            <th>number of channels:</th>
            <th>{{channel_1}}</th>
            <th>{{channel_2}}</th>
          </tr>
          <tr>
            <th>sample rate:</th>
            <th>{{rate_1}} HZ</th>
            <th>{{rate_2}} HZ</th>
